\n","  symbols_list = []\n","  symbols_set = set()\n","  symbols_dict = dict()\n","  for i in text:\n","      symbols_set.add(i)\n","      symbols_list.append(i)\n","  for i in sorted(symbols_set):\n","      symbols_dict[i] = symbols_list.count(i)    # словарь -- символ: его кол-во в тексте\n","  print(f'Имя процесса {mp.current_process().name}')\n","  for i, j in symbols_dict.items():\n","      print(f'{i}:{j}')\n","    "],"metadata":{"id":"fxgh_rb_7dNF"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["processes = []\n","for i in files:\n","  pr = mp.Process(target = count_symb, args = (i,))\n","  processes.append(pr)\n","\n","for p in processes:\n","    p.start()\n","\n","for p in processes:\n","    p.join()   # ждет пока не завершится процесс"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"lyKkXPev2KAR","executionInfo":{"status":"ok","timestamp":1679279320483,"user_tz":-180,"elapsed":4496,"user":{"displayName":"Аня Конева","userId":"05066345844249364087"}},"outputId":"ff8a3f3d-cf92-485e-e965-baf6e8c541df"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Имя процесса Process-4\n","\n",":2734\n"," :45076\n","!:718\n","':59\n","(:276\n","):276\n",",:6372\n","-:900\n",".:2954\n","/:20\n","0:22\n","1:46\n","2:42\n","3:40\n","4:42\n","5:40\n","6:42\n","7:42\n","8:38\n","9:36\n","::212\n",";:406\n","?:571\n","[:97\n","]:97\n","_:4\n","`:9\n","a:590\n","b:220\n","c:324\n","d:192\n","e:1200\n","f:52\n","g:73\n","h:227\n","i:369\n","j:40\n","k:21\n","l:571\n","m:401\n","n:459\n","o:377\n","p:100\n","q:50\n","r:308\n","s:429\n","t:332\n","u:285\n","v:87\n","w:7\n","x:24\n","y:8\n","z:44\n"," :1472\n","«:129\n","»:128\n","а:18236\n","б:3980\n","в:9398\n","г:3948\n","д:6681\n","е:20054\n","ж:2297\n","з:3355\n","и:13587\n","й:2028\n","к:6744\n","л:9961\n","м:7106\n","н:14240\n","о:23130\n","п:5489\n","р:9482\n","с:11507\n","т:14245\n","у:6044\n","ф:634\n","х:1535\n","ц:817\n","ч:4113\n","ш:1943\n","щ:587